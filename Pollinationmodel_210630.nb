   RowBox[{"(", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"1", "-", "H"}], ")"}], "+", "\[Psi]", "-", 
     RowBox[{"2", "\[Psi]", 
      RowBox[{"(", 
       RowBox[{"1", "-", "H"}], ")"}]}]}], ")"}]}]}]], "Input",
 CellChangeTimes->{{3.7688189589315042`*^9, 3.768818980149543*^9}},
 CellLabel->"In[12]:=",ExpressionUUID->"beb98bc4-574b-4c75-9c00-e18eeba1a150"],

